{
	"arrowParens": "always",
	"endOfLine": "lf",
	"printWidth": 100,
	"semi": true,
	"singleQuote": false,
	"tabWidth": 2,
	"trailingComma": "es5",
	"useTabs": false,
	"vueIndentScriptAndStyle": true,
	"tailwindFunctions": ["tv"],
	"importOrder": [
		"<BUILTIN_MODULES>",
		"<THIRD_PARTY_MODULES>",
		"<TYPES>",
		"",
		"^[.]"
	],
	"htmlWhitespaceSensitivity": "ignore",
	"bracketSameLine": true
}
